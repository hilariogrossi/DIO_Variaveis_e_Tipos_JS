let pessoa = {
    nome: 'Patrícia',
    idade: 54
}

pessoa["numeroDeIrmaos"] = 7;
pessoa["nomeDaMae"] = 'Oneida';

console.table(pessoa);
console.log(pessoa.nome);
console.log(pessoa['nome']);
console.log(pessoa['idade']);
console.log(pessoa.numeroDeIrmaos);
console.log(pessoa.nomeDaMae);
